@page "/student"
    <AuthorizeView Roles="@DataBase.Models.Roles.Student.ToString()">
        <NotAuthorized>
            <section class="categories-area section-padding3 section-padding3-bottom">
                <div class="container">
                    <div class="row">
                        <div class="col-lg-12">
                            <h2 class="text-center"> Not Authorized</h2>
                        </div>
                    </div>
                </div>
            </section>
        </NotAuthorized>
        <Authorized>
            <section class="categories-area section-padding0">
                <div class="container">
                    <div class="row">
                        <section class="services-area  services-padding50">
                            <div class="container">
                                <div class="project-heading mb-35">
                                    <div class="row align-items-end">
                                        <div class="col-lg-12">
                                            <!-- Section Tittle -->
                                            <div class="section-tittle">
                                                <h2>Take Your Quizes</h2>
                                            </div>
                                        </div>
                                        <div class="col-lg-12">
                                            <div class="properties__button">
                                                <!--Nav Button  -->
                                                <nav>
                                                    <div class="nav nav-tabs" id="nav-tab" role="tablist">
                                                        <a class="nav-item nav-link active" id="nav-home-tab" data-toggle="tab" href="#nav-home" role="tab" aria-controls="nav-home" aria-selected="false"> Quizes</a>
                                                        <a class="nav-item nav-link" id="nav-profile-tab" data-toggle="tab" href="#nav-profile" role="tab" aria-controls="nav-profile" aria-selected="false"> Results</a>
                                                        <a class="nav-item nav-link" id="nav-contact-tab" data-toggle="tab" href="#nav-contact" role="tab" aria-controls="nav-contact" aria-selected="false">Groups</a>
                                                    </div>
                                                </nav>
                                                <!--End Nav Button  -->
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-12">
                                        <!-- Nav Card -->
                                        <div class="tab-content active" id="nav-tabContent">
                                            <!-- card ONE -->
                                            <div class="tab-pane fade active show" id="nav-home" role="tabpanel" aria-labelledby="nav-home-tab">
                                                <div class="services-caption">
                                                    <div class="row">
                                                        <h1>Tests</h1>
                                                    </div>
                                                </div>
                                            </div>
                                            <!-- Card TWO -->
                                            <div class="tab-pane fade" id="nav-profile" role="tabpanel" aria-labelledby="nav-profile-tab">
                                                <div class="services-caption">
                                                    <div class="row">
                                                        <h3>Results</h3>
                                                    </div>
                                                </div>
                                            </div>
                                            <!-- Card THREE -->
                                            <div class="tab-pane fade" id="nav-contact" role="tabpanel" aria-labelledby="nav-contact-tab">
                                                <div class="services-caption">
                                                    <div class="row">
                                                        <h3>Groups</h3>
                                                    </div>
                                                </div>
                                            </div>
                                           
                                        </div>
                                        <!-- End Nav Card -->
                                    </div>
                                </div>
                            </div>
                        </section>
                    </div>
                </div>
            </section>
        </Authorized>
    </AuthorizeView>
